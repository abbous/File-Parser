TRUNCATE TABLE `<%= table %>`;
<% values.forEach(function(v){ %>
INSERT INTO `<%= table %>` (<%= v.keys.join(',') %>) VALUES (<% var count = 0, len = v.values.length; v.values.forEach(function(i){ count++; %>'<%= i %>'<% if(count < len) { %>,<% } %><% }) %>);<% }); %>